{
	"name": "david63/bulkuseradd",
	"type": "phpbb-extension",
	"description": "Adds the ability to add users from an Excel spreadsheet or a CSV file.",
	"version": "3.1.0-RC1",
	"time": "2020-01-26",
	"license": "GPL-2.0-only",
	"authors": [
		{
			"name": "David Wood",
			"role": "Developer"
		},
		{
            "name": "PHPOffice",
            "homepage": "https://github.com/PHPOffice/PhpSpreadsheet",
            "role": "Developer - PhpSpreadsheet"
        }
    ],
	"require": {
        "php": ">=7.1.3",
		"phpoffice/phpspreadsheet": "^1.5"
    },
	"extra": {
		"display-name": "[**] Bulk User Add",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0"
		},
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions",
        	"filename": "bulk_user_add_version_file.json",
			"ssl": false
    	}
	}
}
